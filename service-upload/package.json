{
  "name": "service-upload",
  "description": "Upload service for fetching video from gcp bucket and posting to integration APIS",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start:dev": "NODE_ENV=development node index.js",
    "start": "node index.js",
    "test": "vitest",
    "build": "tsc --p tsconfig.json",
    "postinstall": "npm run build",
    "deploy": "npm run build && gcloud run deploy service-upload  --source ."
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "Google LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google-cloud/storage": "5.15.0",
    "@prisma/client": "^4.15.0",
    "@t_g/default-ui": "^0.26.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.17.1",
    "googleapis": "^118.0.0",
    "lodash": "^4.17.21",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.199",
    "nodemon": "^2.0.22",
    "vitest": "^0.23.4"
  }
}
